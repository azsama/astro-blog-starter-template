---
import {SITE_TITLE, SITE_DESCRIPTION} from '../consts';
const API_URL = 'https://ms1.wangxiansen.top/api/realtime/quotec?symbol=SZ300757';

// 每一次请求都强制抓最新数据，避免 Vercel/CF Edge 缓存
const response = await fetch(API_URL, {cache: 'no-store'});
const resp = await response.json() as any;
const stock = JSON.stringify(resp, null, 2)
---

<html lang="zh-CN">
<head>
  <meta charset="utf-8" />
</head>
<pre set:html={stock}></pre>
</html>
